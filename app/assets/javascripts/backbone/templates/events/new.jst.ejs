<h1>New event</h1>

<form id="new-event" name="event">
  <div class="field">
    <label for="post"> post:</label>
    <input type="text" name="post" id="post" value="<%= post %>" >
  </div>

  <div class="field">
    <label for="created_at"> created_at:</label>
    <input type="text" name="created_at" id="created_at" value="<%= created_at %>" >
  </div>

  <div class="field">
    <label for="updated_at"> updated_at:</label>
    <input type="text" name="updated_at" id="updated_at" value="<%= updated_at %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Events" />
  </div>

</form>

